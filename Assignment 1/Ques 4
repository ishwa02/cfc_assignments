import java.util.*;
public class A1P4{

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int odd =1,even=2;
        System.out.println("enter a number of elements you want to print from the series");
        int n = sc.nextInt();
       
       while(n!=0)
       { 
           System.out.print(even + ", ");
           even=even+(2*(odd+1));
           odd = odd +2;
           n--;
           
      }
       
    }
}
